{
	"name": "gcov-visualization",
	"displayName": "gcov-visualization",
	"description": "gcov visualization",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.toggleVisualization"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.toggleVisualization",
				"title": "Toggle Gcov"
			}
		],
		"keybindings": [
			{
				"command": "extension.toggleVisualization",
				"key": "ctrl+f1",
				"mac": "cmd+f1",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"ls" : "ls",
		"cwd" : "cwd",
		"copy": "cp ./src/test/suite/testfiles/*/* ./out"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"cpx": "^1.5.0",
		"cwd": "^0.10.0",
		"decache": "^4.5.1",
		"istanbul": "^0.4.5",
		"remap-istanbul": "^0.13.0",
		"utils": "^0.3.1"
	}
}
